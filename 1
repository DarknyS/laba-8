#include <iostream>
#include <math.h>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	double a, b;
	cout << "Введите а,b" << endl;
	cin >> a >> b;
	if (a == b) {
		b = 0;
		a = 0;
		cout << "a=" << a << " " << "b=" << b << endl;
	}
	if (a > b) {
		b = a;
		cout << "a=" << a << " " << "b=" << b << endl;
	}
	if (b > a) {
		a = b;
		cout << "a=" << a << " " << "b=" << b << endl;
	}
}
